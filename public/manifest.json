{
  "manifest_version": 3,
  "name": "TrustFactor",
  "version": "1.0",
  "description": "Accept the terms and conditions confidently.",
  "permissions": [
    "activeTab",
    "scripting",
    "sidePanel"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": "icon.png"
  },
  "side_panel": {
    "default_path": "sidebar.html"
  },
  "icons": {
    "48": "icon.png"
  }
}